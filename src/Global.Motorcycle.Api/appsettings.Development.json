{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Delivery": {
    "Uri": "https://localhost:7084/api/"
  },
  "Kafka": {
    "Server": "localhost:29092",
    "Motorcycle": {
      "CreateTopic": "global-motorcycle-created",
      "UpdateTopic": "global-motorcycle-updated",
      "DeleteTopic": "global-motorcycle-deleted"
    },
    "Location": {
      "CreateTopic": "global-location-created",
      "ReturnedTopic": "global-location-returned"
    }
  },
  "Cache": {
    "Key": "Motorcycles",
    "MinutesExpiration": 5
  },
  "ConnectionStrings": {
    "MotorcycleManagement": "Host=localhost;Port=5432;Database=MotorcycleDB;Username=desenv;Password=password@desenv",
    "Redis": "localhost:6379"
  }
}
